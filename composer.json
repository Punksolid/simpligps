{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "arcanedev/log-viewer": "^4.5",
    "barryvdh/laravel-cors": "^0.11.2",
    "darkaonline/l5-swagger": "^5.7",
    "fideloper/proxy": "^4.0",
    "hyn/multi-tenant": "5.4.0-beta.2",
    "laravel/cashier": "~7.0",
    "laravel/framework": "5.8.*",
    "laravel/passport": "^7.0",
    "laravel/telescope": "v2.0.2",
    "laravel/tinker": "^1.0",
    "maatwebsite/excel": "^2.1",
    "mpociot/laravel-apidoc-generator": "^2.0",
    "predis/predis": "^1.1",
    "punksolid/laravel-wialon": "dev-master",
    "pusher/pusher-php-server": "~3.0",
    "spatie/geocoder": "^3.4",
    "spatie/laravel-permission": "^2.12",
    "spatie/laravel-tags": "^2.0",
    "tymon/jwt-auth": "^1.0",
    "zircote/swagger-php": "3.*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.4",
    "beyondcode/laravel-er-diagram-generator": "^1.1",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "itsgoingd/clockwork": "^3.0",
    "laravel/telescope": "^2.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories",
      "database/seeds/tenants"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "hyn/multi-tenant"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
